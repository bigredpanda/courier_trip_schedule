body
  background #332525

#navigation
  width 1000px
  margin auto
  margin-top 10%
  text-align center
  font-size 1.6em
  color #FFF
  font-weight bold
  text-shadow: 2px 1px 3px rgba(0, 0, 0, 1);
  a
    color #FFF
    font-weight bold
    text-shadow: 2px 1px 3px rgba(0, 0, 0, 1);

#content
  background #fff
  padding 15px
  width 1000px
  min-height 100px
  margin auto
  margin-top: 1%
  box-shadow 0 0 20px 0 #000
  -webkit-box-shadow 0 0 20px 0 #000

#addButton
  margin-bottom 10px
  font-size 15px
  font-weight bold
  border-radius 50%

.deleteBtn
  font-weight bold
  border-radius 50%

#filter
  border 1px solid #bebebe
  margin-bottom 10px

.table
  thead
    background-color #337ab7
    color #FFF

.table-bordered>thead>tr>td,
.table-bordered>thead>tr>th
  border-bottom-width 1px